{
  "name": "flexar-support-for-vscode",
  "displayName": "Flexar Support for VSCode",
  "description": "The Oficial Support For Flexar Programming Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "flexar",
      "aliases": ["flexar", "flexar"],
      "extensions": [".fl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "flexar",
      "scopeName": "source.fl",
      "path": "./syntaxes/flexar.tmLanguage.json"
    }]
  }
}
